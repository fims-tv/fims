  <!-- ****************************** FIMS-Defined Simple Types ****************************** -->
  <simpleType name="UID">
    <annotation>
      <documentation source="urn:x-fims:description">
        Unique identifiers providing a generic representation 
        for UUIDs, ULs and UMIDs.
      </documentation>
      <documentation source="urn:x-fims:normativeRequirement">
        Although empty IDs are permitted, these should only be used for initial requests 
        where the server is expected to create an identifier.
      </documentation>
      <documentation source="urn:x-fims:modifiedInVersion">Modified In Version: 1_1_0</documentation>
    </annotation>
    <!--<union memberTypes="bms:UUID bms:UMID bms:UL bms:EmptyID"/> V1_0_7-->
    <restriction base="string">
      <pattern value="(urn:uuid:)?[0-9a-fA-F]{8}-[0-9a-fA-F]{4}-[0-9a-fA-F]{4}-[0-9a-fA-F]{4}-[0-9a-fA-F]{12}|urn:smpte:umid:([0-9a-fA-F]{8}\.){7}[0-9a-fA-F]{8}|urn:smpte:ul:([0-9a-fA-F]{8}\.){3}[0-9a-fA-F]{8}|"/>
    </restriction>
  </simpleType>

  <simpleType name="ResourceIDType">
    <annotation>
      <documentation source="urn:x-fims:description">
