<schema xmlns="http://www.w3.org/2001/XMLSchema" xmlns:bms="http://base.fims.tv"
	xmlns:desc="http://description.fims.tv" xmlns:tim="http://baseTime.fims.tv"
	targetNamespace="http://base.fims.tv"
	elementFormDefault="qualified">
  <import namespace="http://description.fims.tv" schemaLocation="description-V1_1_0.xsd"/>
  <import namespace="http://baseTime.fims.tv" schemaLocation="baseTime-V1_1_0.xsd"/>
  <!-- ****************************** FIMS Current Version ****************************** -->
  <simpleType name="CurrentVersion">
    <annotation>
      <documentation source="urn:x-fims:description">
        This schema type reports the current version of the Base Media Service schema for Clients 
        to use to determine the current feature set.
      </documentation>
      <documentation source="urn:x-fims:normativeRequirement"></documentation>
      <documentation source="urn:x-fims:serviceDescription"></documentation>
      <documentation source="urn:x-fims:contentOfServiceDescription"></documentation>
      <documentation source="urn:x-fims:introducedInVersion">Introduced In Version: 1_1_0</documentation>
    </annotation>
    <restriction base="string">
      <enumeration value="1_1_0">
        <annotation>
          <documentation source="urn:x-fims:description">
            This is the current FIMS version. Clients should use this value when determining 
            what features are available in this version.
          </documentation>
        </annotation>
      </enumeration>
    </restriction>
  </simpleType>
  <!-- ****************************** Extension Types ****************************** -->
  <complexType name="ExtensionGroup">
    <annotation>
      <documentation source="urn:x-fims:description">
        Each Type includes an ExtensionGroup to allow for vendor-specific extensions, 
        but the definition is out of scope of the FIMS specification.
      </documentation>
      <documentation source="urn:x-fims:normativeRequirement"/>
      <documentation source="urn:x-fims:serviceDescription"/>
      <documentation source="urn:x-fims:contentOfServiceDescription"/>
    </annotation>
    <sequence>
      <any namespace="##other" processContents="lax" minOccurs="0" maxOccurs="unbounded">
        <annotation>
          <documentation source="urn:x-fims:description">
            Any number of extension elements from an external namespace. 
            The lax processing model indicates that if the schema for the extension 
            elements is known, then they should be validated. If the schema is not known, 
            no error will be generated. Extension nodes must be the last element in a 
            sequence of a non-abstract type.
          </documentation>
          <documentation source="urn:x-fims:normativeRequirement"/>
          <documentation source="urn:x-fims:serviceDescription"/>
          <documentation source="urn:x-fims:contentOfServiceDescription"/>
          <documentation source="urn:x-fims:inclusionInRequest">Inclusion In Request: Optional</documentation>
          <documentation source="urn:x-fims:inclusionInResponse">Inclusion In Response: Optional</documentation>
        </annotation>
      </any>
    </sequence>
  </complexType>
  <complexType name="ExtensionAttributes">
    <annotation>
      <documentation source="urn:x-fims:description">
        Each Type includes an ExtensionAttributes to allow for vendor-specific extensions, 
        but the definition is out of scope of the FIMS specification.
      </documentation>
      <documentation source="urn:x-fims:normativeRequirement"/>
      <documentation source="urn:x-fims:serviceDescription"/>
      <documentation source="urn:x-fims:contentOfServiceDescription"/>
    </annotation>
    <anyAttribute namespace="##other" processContents="lax">
      <annotation>
        <documentation source="urn:x-fims:description">
          Permitted extensions to elements that permit extensions. Attributes from other namespaces are permitted. Lax processing requests that a validator checks the attributes where access to the attribute's defining schema is available.
        </documentation>
        <documentation source="urn:x-fims:normativeRequirement">
          Extension attributes should not be used where an equivalent property is available in the FIMS data model.
          Attributes from the FIMS base namespace shall not be used.
        </documentation>
        <documentation source="urn:x-fims:serviceDescription"/>
        <documentation source="urn:x-fims:contentOfServiceDescription"/>
        <documentation source="urn:x-fims:inclusionInRequest">Inclusion In Request: Optional</documentation>
        <documentation source="urn:x-fims:inclusionInResponse">Inclusion In Response: Optional</documentation>
      </annotation>
    </anyAttribute>
  </complexType>
  <!-- ****************************** FIMS-Defined References ****************************** -->
  <complexType name="ResourceReferenceType">
    <annotation>
      <documentation source="urn:x-fims:description">
        The source or target of a reference to a distinct resource. 
        Resources include queues, jobs, assets, descriptions, formats, and profiles.
      </documentation>
      <documentation source="urn:x-fims:normativeRequirement"/>
      <documentation source="urn:x-fims:serviceDescription"/>
      <documentation source="urn:x-fims:contentOfServiceDescription"/>
    </annotation>
    <sequence>
      <element name="resourceID" type="bms:ResourceIDType" minOccurs="1" maxOccurs="1">
        <annotation>
          <documentation source="urn:x-fims:description">
            Locally distinct identifier for the resource that can be used to make reference to the resource. The
            reference may also be globally unique.
          </documentation>
          <documentation source="urn:x-fims:normativeRequirement"/>
          <documentation source="urn:x-fims:serviceDescription"/>
          <documentation source="urn:x-fims:contentOfServiceDescription"/>
          <documentation source="urn:x-fims:inclusionInRequest">Inclusion In Request: Mandatory</documentation>
          <documentation source="urn:x-fims:inclusionInResponse">Inclusion In Response: Mandatory</documentation>
        </annotation>
      </element>
      <element name="revisionID" type="bms:RevisionIDType" minOccurs="0" maxOccurs="1">
        <annotation>
          <documentation source="urn:x-fims:description">
            Identifier for the specific revision of the resource. Tracking the different versions of resources
            across systems enables different instances and versions of a resource to
            coexist at different locations and updated to be eventually consistent
            according to local policy.
          </documentation>
          <documentation source="urn:x-fims:normativeRequirement">
            A reference to a resource that omits the revision identification of the resource shall
            indicate that the latest version is required.
          </documentation>
          <documentation source="urn:x-fims:serviceDescription">Service Description: Required</documentation>
          <documentation source="urn:x-fims:contentOfServiceDescription">Description: Support or not.</documentation>
          <documentation source="urn:x-fims:inclusionInRequest">Inclusion In Request: Optional</documentation>
          <documentation source="urn:x-fims:inclusionInResponse">Inclusion In Response: Optional</documentation>
        </annotation>
      </element>
      <element name="location" type="anyURI" minOccurs="0" maxOccurs="1">
        <annotation>
          <documentation source="urn:x-fims:description">
            A Uniform Resource Location that points to a specific instance of a resource at a location.
          </documentation>
          <documentation source="urn:x-fims:normativeRequirement">
            Reference to a resource that omits the location shall require resolution of the resourceID according
            to local policy.
          </documentation>
          <documentation source="urn:x-fims:serviceDescription">Service Description: Required</documentation>
          <documentation source="urn:x-fims:contentOfServiceDescription">Description: Support or not.</documentation>
          <documentation source="urn:x-fims:inclusionInRequest">Inclusion In Request: Optional</documentation>
          <documentation source="urn:x-fims:inclusionInResponse">Inclusion In Response: Optional</documentation>
        </annotation>
      </element>
    </sequence>
  </complexType>
  <!-- ****************************** FIMS-Defined Resources ****************************** -->
  <complexType name="ResourceType" abstract="true">
    <annotation>
      <documentation source="urn:x-fims:description">
        Resources include queues, jobs, assets, descriptions, formats, and profiles.
      </documentation>
      <documentation source="urn:x-fims:normativeRequirement"/>
      <documentation source="urn:x-fims:serviceDescription"/>
      <documentation source="urn:x-fims:contentOfServiceDescription"/>
    </annotation>
    <complexContent>
      <extension base="bms:ResourceReferenceType">
        <sequence>
          <element name="resourceCreationDate" type="dateTime" minOccurs="0" maxOccurs="1">
            <annotation>
              <documentation source="urn:x-fims:description">
                Date and time when this resource was created.
              </documentation>
              <documentation source="urn:x-fims:normativeRequirement"/>
              <documentation source="urn:x-fims:serviceDescription"/>
              <documentation source="urn:x-fims:contentOfServiceDescription"/>
              <documentation source="urn:x-fims:inclusionInRequest">Inclusion In Request: Optional</documentation>
              <documentation source="urn:x-fims:inclusionInResponse">Inclusion In Response: Optional</documentation>
            </annotation>
          </element>
          <element name="resourceModifiedDate" type="dateTime" minOccurs="0" maxOccurs="1">
            <annotation>
              <documentation source="urn:x-fims:description">
                Date and time when this resource was last modified. Modification date of the resource should
                be updated when any of the values of the resource's properties are updated.
              </documentation>
              <documentation source="urn:x-fims:normativeRequirement"/>
              <documentation source="urn:x-fims:serviceDescription"/>
              <documentation source="urn:x-fims:contentOfServiceDescription"/>
              <documentation source="urn:x-fims:inclusionInRequest">Inclusion In Request: Optional</documentation>
              <documentation source="urn:x-fims:inclusionInResponse">Inclusion In Response: Optional</documentation>
            </annotation>
          </element>
          <element name="serviceGeneratedElement" type="boolean" minOccurs="0" maxOccurs="1">
            <annotation>
              <documentation source="urn:x-fims:description">
                An indication the service has generated the object as a result of an operation or procedure.
              </documentation>
              <documentation source="urn:x-fims:normativeRequirement"/>
              <documentation source="urn:x-fims:serviceDescription"/>
              <documentation source="urn:x-fims:contentOfServiceDescription"/>
              <documentation source="urn:x-fims:inclusionInRequest">Inclusion In Request: No</documentation>
              <documentation source="urn:x-fims:inclusionInResponse">Inclusion In Response: Optional</documentation>
              <documentation source="urn:x-fims:introducedInVersion">Introduced In Version: 1_1_0</documentation>
            </annotation>
          </element>
          <element name="isFullyPopulated" type="boolean" minOccurs="0" maxOccurs="1">
            <annotation>
              <documentation source="urn:x-fims:description">
                An indication the service has fully populated the generated object.
              </documentation>
              <documentation source="urn:x-fims:normativeRequirement"/>
              <documentation source="urn:x-fims:serviceDescription"/>
              <documentation source="urn:x-fims:contentOfServiceDescription"/>
              <documentation source="urn:x-fims:inclusionInRequest">Inclusion In Request: Optional</documentation>
              <documentation source="urn:x-fims:inclusionInResponse">Inclusion In Response: Optional</documentation>
              <documentation source="urn:x-fims:introducedInVersion">Introduced In Version: 1_1_0</documentation>
            
            </annotation>
          </element>
          
           <element name="notifyAt" type="bms:AsyncEndpointType" minOccurs="0"
						maxOccurs="1">
            <annotation>
              <documentation source="urn:x-fims:description">
                Endpoints where a service can send back a notification for a job completed or failed.
              </documentation>
              <documentation source="urn:x-fims:normativeRequirement">
                A service shall support either notification or polling. If this element is set, the
                service shall send a notification to the address(es) indicated in
                the "replyTo" field(s) for successful completion and shall send a
                notification to the address(es) indicated in the "faultTo" field(s)
                for failure. If this element is not set, the service shall not send
                notifications to the requester.
              </documentation>
              <documentation source="urn:x-fims:serviceDescription">Service Description: Required</documentation>
              <documentation source="urn:x-fims:contentOfServiceDescription">Description: 
                Support or not, including the maximum number of notification endpoints.
              </documentation>
              <documentation source="urn:x-fims:inclusionInRequest">Inclusion In Request: Optional</documentation>
              <documentation source="urn:x-fims:inclusionInResponse">Inclusion In Response: Not applicable</documentation>
            </annotation>
          </element>
          <element name="ExtensionGroup" type="bms:ExtensionGroup" minOccurs="0"
						maxOccurs="1">
            <annotation>
              <documentation source="urn:x-fims:description">Extension point.</documentation>
              <documentation source="urn:x-fims:normativeRequirement"/>
              <documentation source="urn:x-fims:serviceDescription"/>
              <documentation source="urn:x-fims:contentOfServiceDescription"/>
              <documentation source="urn:x-fims:inclusionInRequest"/>
              <documentation source="urn:x-fims:inclusionInResponse"/>
            </annotation>
          </element>
          <element name="ExtensionAttributes" type="bms:ExtensionAttributes" minOccurs="0"
						maxOccurs="1">
            <annotation>
              <documentation source="urn:x-fims:description">Extension point.</documentation>
              <documentation source="urn:x-fims:normativeRequirement"/>
              <documentation source="urn:x-fims:serviceDescription"/>
              <documentation source="urn:x-fims:contentOfServiceDescription"/>
              <documentation source="urn:x-fims:inclusionInRequest"/>
              <documentation source="urn:x-fims:inclusionInResponse"/>
            </annotation>
          </element>
        </sequence>
      </extension>
    </complexContent>
  </complexType>
  
  
  <complexType name="ExtensionBaseType" abstract="true">
    <annotation>
      <documentation source="urn:x-fims:description">
        Abstract type containing the extension group and extension attributes concepts. 
      </documentation>
      <documentation source="urn:x-fims:normativeRequirement"/>
      <documentation source="urn:x-fims:serviceDescription"/>
      <documentation source="urn:x-fims:contentOfServiceDescription"/>
    </annotation>
    
    
    <sequence>    
      <element name="ExtensionGroup" type="bms:ExtensionGroup" minOccurs="0"
        maxOccurs="1">
        <annotation>
          <documentation source="urn:x-fims:description">Extension point.</documentation>
          <documentation source="urn:x-fims:normativeRequirement"/>
          <documentation source="urn:x-fims:serviceDescription"/>
          <documentation source="urn:x-fims:contentOfServiceDescription"/>
          <documentation source="urn:x-fims:inclusionInRequest"/>
          <documentation source="urn:x-fims:inclusionInResponse"/>
        </annotation>
      </element>
      <element name="ExtensionAttributes" type="bms:ExtensionAttributes" minOccurs="0"
        maxOccurs="1">
        <annotation>
          <documentation source="urn:x-fims:description">Extension point.</documentation>
          <documentation source="urn:x-fims:normativeRequirement"/>
          <documentation source="urn:x-fims:serviceDescription"/>
          <documentation source="urn:x-fims:contentOfServiceDescription"/>
          <documentation source="urn:x-fims:inclusionInRequest"/>
          <documentation source="urn:x-fims:inclusionInResponse"/>
        </annotation>
      </element>
    </sequence>
    
    
  </complexType>
  
  
  
  
  
  <complexType name="AsyncEndpointType">
    <annotation>
      <documentation source="urn:x-fims:description">
        Provides endpoints where a service can send back a notification for a job completed or failed.
      </documentation>
      <documentation source="urn:x-fims:normativeRequirement"/>
      <documentation source="urn:x-fims:serviceDescription"/>
      <documentation source="urn:x-fims:contentOfServiceDescription"/>
    </annotation>
    <sequence>
      <element name="replyTo" type="anyURI" minOccurs="1" maxOccurs="1">
        <annotation>
          <documentation source="urn:x-fims:description">
            Endpoint where the service sends notifications indicating the successful 
            termination or cancellation of a job. Depending on the invoking client 
            or business process, the endpoint for "faultTo" can be the same as the 
            "replyTo" or it can be different.
          </documentation>
          <documentation source="urn:x-fims:normativeRequirement"/>
          <documentation source="urn:x-fims:serviceDescription"/>
          <documentation source="urn:x-fims:contentOfServiceDescription"/>
          <documentation source="urn:x-fims:inclusionInRequest">Inclusion In Request: Mandatory</documentation>
          <documentation source="urn:x-fims:inclusionInResponse">Inclusion In Response: Mandatory</documentation>
        </annotation>
      </element>
      <element name="faultTo" type="anyURI" minOccurs="1" maxOccurs="1">
        <annotation>
          <documentation source="urn:x-fims:description">
            Endpoint where the service sends the fault notification indicating the failure of a job. 
            Depending on the invoking client or business process, the endpoint for "faultTo" can be the
            same as the "replyTo" or it can be different.
          </documentation>
          <documentation source="urn:x-fims:normativeRequirement"/>
          <documentation source="urn:x-fims:serviceDescription"/>
          <documentation source="urn:x-fims:contentOfServiceDescription"/>
          <documentation source="urn:x-fims:inclusionInRequest">Inclusion In Request: Mandatory</documentation>
          <documentation source="urn:x-fims:inclusionInResponse">Inclusion In Response: Mandatory</documentation>
        </annotation>
      </element>
    </sequence>
  </complexType>
  <complexType name="ServiceType">
    <annotation>
      <documentation source="urn:x-fims:description">
        Describes the location endpoint, provider and Service Description of the service.
      </documentation>
      <documentation source="urn:x-fims:normativeRequirement"/>
      <documentation source="urn:x-fims:serviceDescription"/>
      <documentation source="urn:x-fims:contentOfServiceDescription"/>
    </annotation>
    <complexContent>
      <extension base="bms:ResourceType">
        <sequence>
          <element name="providerName" type="string" minOccurs="0" maxOccurs="1">
            <annotation>
              <documentation source="urn:x-fims:description">
                Name for the provider of a service.
              </documentation>
              <documentation source="urn:x-fims:normativeRequirement"/>
              <documentation source="urn:x-fims:serviceDescription">Service Description: Required</documentation>
              <documentation source="urn:x-fims:contentOfServiceDescription">Description: Support or not</documentation>
              <documentation source="urn:x-fims:inclusionInRequest">Inclusion In Request: Optional</documentation>
              <documentation source="urn:x-fims:inclusionInResponse">Inclusion In Response: Optional</documentation>
            </annotation>
          </element>
          <element name="providerEndPoint" type="anyURI" minOccurs="0" maxOccurs="1">
            <annotation>
              <documentation source="urn:x-fims:description">
                Provider endpoint that uniquely locates the provider servicing the job request.
              </documentation>
              <documentation source="urn:x-fims:normativeRequirement"/>
              <documentation source="urn:x-fims:serviceDescription"/>
              <documentation source="urn:x-fims:contentOfServiceDescription"/>
              <documentation source="urn:x-fims:inclusionInRequest">Inclusion In Request: Optional</documentation>
              <documentation source="urn:x-fims:inclusionInResponse">Inclusion In Response: Mandatory</documentation>
            </annotation>
          </element>
          <element name="serviceDescription" minOccurs="0" maxOccurs="1">
            <annotation>
              <documentation source="urn:x-fims:description">
                Service-specific description of the capabilities offered by the service. The
                transfer, transcode and capture services defined alongside the
                original FIMS framework have service descriptions provided and, if
                used, it is recommended that these are embedded here.
              </documentation>
              <documentation source="urn:x-fims:normativeRequirement"/>
              <documentation source="urn:x-fims:serviceDescription"/>
              <documentation source="urn:x-fims:contentOfServiceDescription"/>
              <documentation source="urn:x-fims:inclusionInRequest">Inclusion In Request: Not applicable</documentation>
              <documentation source="urn:x-fims:inclusionInResponse">Inclusion In Response: Optional</documentation>
            </annotation>
            <complexType>
              <sequence>
                <any namespace="##other" processContents="lax" minOccurs="1" maxOccurs="unbounded">
                  <annotation>
                    <documentation source="urn:x-fims:description">This is allows for any future elements to be backward compatible.</documentation>
                  </annotation>
                </any>
              </sequence>
            </complexType>
          </element>
        </sequence>
      </extension>
    </complexContent>
  </complexType>
  <complexType name="ProfileType" abstract="true">
    <annotation>
      <documentation source="urn:x-fims:description">
        A profile used by a FIMS media service to perform a job on media content. For example, it may represent the profile of a
        transfer media service for transferring media content and, as such, may specify the
        media format to be produced in output. The profile provides a mechanism to specify
        service-provider-specific information for each operation.
      </documentation>
      <documentation source="urn:x-fims:normativeRequirement"/>
      <documentation source="urn:x-fims:serviceDescription"/>
      <documentation source="urn:x-fims:contentOfServiceDescription"/>
    </annotation>
    <complexContent>
      <extension base="bms:ResourceType">
        <sequence>
          <element name="service" type="bms:ServiceType" minOccurs="0" maxOccurs="1">
            <annotation>
              <documentation source="urn:x-fims:description">
                Details of the service that has been selected to execute this profile.
              </documentation>
              <documentation source="urn:x-fims:normativeRequirement"/>
              <documentation source="urn:x-fims:serviceDescription"/>
              <documentation source="urn:x-fims:contentOfServiceDescription"/>
              <documentation source="urn:x-fims:inclusionInRequest">Inclusion In Request: Optional</documentation>
              <documentation source="urn:x-fims:inclusionInResponse">Inclusion In Response: Optional</documentation>
            </annotation>
          </element>
        </sequence>
        <attribute name="name" type="string" use="optional">
          <annotation>
            <documentation source="urn:x-fims:description">Name of the profile.</documentation>
            <documentation source="urn:x-fims:normativeRequirement"/>
            <documentation source="urn:x-fims:serviceDescription"/>
            <documentation source="urn:x-fims:contentOfServiceDescription"/>
            <documentation source="urn:x-fims:inclusionInRequest">Inclusion In Request: Optional</documentation>
            <documentation source="urn:x-fims:inclusionInResponse">Inclusion In Response: Optional</documentation>
          </annotation>
        </attribute>
        <attribute name="description" type="string" use="optional">
          <annotation>
            <documentation source="urn:x-fims:description">Description of the profile.</documentation>
            <documentation source="urn:x-fims:normativeRequirement"/>
            <documentation source="urn:x-fims:serviceDescription"/>
            <documentation source="urn:x-fims:contentOfServiceDescription"/>
            <documentation source="urn:x-fims:inclusionInRequest">Inclusion In Request: Optional</documentation>
            <documentation source="urn:x-fims:inclusionInResponse">Inclusion In Response: Optional</documentation>
          </annotation>
        </attribute>
      </extension>
    </complexContent>
  </complexType>
  <complexType name="QueueType">
    <annotation>
      <documentation source="urn:x-fims:description">
        Provides basic queue information, such as the status and length of the queue.
      </documentation>
      <documentation source="urn:x-fims:normativeRequirement"/>
      <documentation source="urn:x-fims:serviceDescription">Service Description: Required</documentation>
      <documentation source="urn:x-fims:contentOfServiceDescription">Description: 
        Support or not the queue, queue depth, and supported queue commands
      </documentation>
    </annotation>
    <complexContent>
      <extension base="bms:ResourceType">
        <sequence>
          <element name="status" type="bms:QueueStatusType" minOccurs="0" maxOccurs="1">
            <annotation>
              <documentation source="urn:x-fims:description">
                Status information for the queue.
              </documentation>
              <documentation source="urn:x-fims:normativeRequirement"/>
              <documentation source="urn:x-fims:serviceDescription"/>
              <documentation source="urn:x-fims:contentOfServiceDescription"/>
              <documentation source="urn:x-fims:inclusionInRequest">Inclusion In Request: Not applicable</documentation>
              <documentation source="urn:x-fims:inclusionInResponse">Inclusion In Response: Mandatory</documentation>
            </annotation>
          </element>
          <element name="statusDescription" type="string" minOccurs="0" maxOccurs="1">
            <annotation>
              <documentation source="urn:x-fims:description">
                An optional description for the status.
              </documentation>
              <documentation source="urn:x-fims:normativeRequirement"/>
              <documentation source="urn:x-fims:serviceDescription"/>
              <documentation source="urn:x-fims:contentOfServiceDescription"/>
              <documentation source="urn:x-fims:inclusionInRequest">Inclusion In Request: Not applicable</documentation>
              <documentation source="urn:x-fims:inclusionInResponse">Inclusion In Response: Optional</documentation>
            </annotation>
          </element>
          <element name="length" type="nonNegativeInteger" minOccurs="0" maxOccurs="1">
            <annotation>
              <documentation source="urn:x-fims:description">
                Length of the queue, measured in the number of jobs currently in this
                queue.
              </documentation>
              <documentation source="urn:x-fims:normativeRequirement"/>
              <documentation source="urn:x-fims:serviceDescription"/>
              <documentation source="urn:x-fims:contentOfServiceDescription"/>
              <documentation source="urn:x-fims:inclusionInRequest">Inclusion In Request: Not applicable</documentation>
              <documentation source="urn:x-fims:inclusionInResponse">Inclusion In Response: Mandatory</documentation>
            </annotation>
          </element>
          <element name="availability" type="boolean" minOccurs="0" maxOccurs="1">
            <annotation>
              <documentation source="urn:x-fims:description">
                Determines if the queue is currently available.
              </documentation>
              <documentation source="urn:x-fims:normativeRequirement"/>
              <documentation source="urn:x-fims:serviceDescription"/>
              <documentation source="urn:x-fims:contentOfServiceDescription"/>
              <documentation source="urn:x-fims:inclusionInRequest">Inclusion In Request: Not applicable</documentation>
              <documentation source="urn:x-fims:inclusionInResponse">Inclusion In Response: Mandatory</documentation>
            </annotation>
          </element>
          <element name="estimatedTotalCompletionDuration" type="duration" minOccurs="0"
						maxOccurs="1">
            <annotation>
              <documentation source="urn:x-fims:description">
                Estimate of the time duration required for processing all jobs currently in the
                queue.
              </documentation>
              <documentation source="urn:x-fims:normativeRequirement"/>
              <documentation source="urn:x-fims:serviceDescription">Service Description: Required</documentation>
              <documentation source="urn:x-fims:contentOfServiceDescription">Description: Support or not</documentation>
              <documentation source="urn:x-fims:inclusionInRequest">Inclusion In Request: Not applicable</documentation>
              <documentation source="urn:x-fims:inclusionInResponse">Inclusion In Response: Optional</documentation>
            </annotation>
          </element>
          <element name="jobs" type="bms:JobsType" minOccurs="0" maxOccurs="1">
            <annotation>
              <documentation source="urn:x-fims:description">
                Optional list of jobs currently in the queue.
              </documentation>
              <documentation source="urn:x-fims:normativeRequirement"/>
              <documentation source="urn:x-fims:serviceDescription"/>
              <documentation source="urn:x-fims:contentOfServiceDescription"/>
