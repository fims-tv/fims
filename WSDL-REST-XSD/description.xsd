<schema xmlns="http://www.w3.org/2001/XMLSchema" xmlns:desc="http://description.fims.tv"
	xmlns:tim="http://baseTime.fims.tv" targetNamespace="http://description.fims.tv"
	elementFormDefault="qualified">
	<import namespace="http://www.w3.org/XML/1998/namespace" schemaLocation="xml.xsd"/>
	<import namespace="http://baseTime.fims.tv" schemaLocation="baseTime.xsd"/>
	<!--**Import new version-->
	<element name="bmContentDescription" type="desc:bmContentDescriptionType"/>
	<complexType name="bmContentDescriptionType">
		<annotation>
			<documentation source="urn:x-fims:description" xml:lang="en">The body of descriptive
				metadata</documentation>
			<documentation source="urn:x-fims:normativeRequirement"/>
			<documentation source="urn:x-fims:serviceDescription"/>
			<documentation source="urn:x-fims:contentOfServiceDescription"/>
			<documentation source="urn:x-fims:introducedInVersion">Introduced In Version:
				1_0_7</documentation>
		</annotation>
		<sequence>
			<element name="title" type="desc:titleType" minOccurs="0" maxOccurs="unbounded">
				<annotation>
					<documentation source="urn:x-fims:description" xml:lang="en">A Title is the
						‘main’ name given to a resource e.g. an editorial object. It corresponds for
						a series to the series title, for programme to the programme title, for an
						item to the item title, etc. Titles are recorded as they appear. The Title
						is the name by which a resource is formally known and that everyone should
						use to refer to or search for that particular resource. The Title may be
						provided in several languages. </documentation>
					<documentation source="urn:x-fims:normativeRequirement"/>
					<documentation source="urn:x-fims:serviceDescription"/>
					<documentation source="urn:x-fims:contentOfServiceDescription"/>
					<documentation source="urn:x-fims:introducedInVersion">Introduced In Version:
						1_0_7</documentation>
				</annotation>
			</element>
			<element name="alternativeTitle" type="desc:alternativeTitleType" minOccurs="0"
				maxOccurs="unbounded">
				<annotation>
					<documentation source="urn:x-fims:description" xml:lang="en">An Alternative
						Title is the name other than the ‘main’ Title given to a resource. The type
						of title is defined by the typeGroup of attributes. Alternative Titles are
						recorded as they appear. An Alternative Title may be attributed to a
						resource for several reasons described using the type (e.g. series title)
						attributes. The alternativeTitle may be provided in several
						languages.</documentation>
					<documentation source="urn:x-fims:normativeRequirement"/>
					<documentation source="urn:x-fims:serviceDescription"/>
					<documentation source="urn:x-fims:contentOfServiceDescription"/>
					<documentation source="urn:x-fims:introducedInVersion">Introduced In Version:
						1_0_7</documentation>
				</annotation>
			</element>
			<element name="creator" type="desc:entityType" minOccurs="0" maxOccurs="unbounded">
				<annotation>
					<documentation source="urn:x-fims:description" xml:lang="en">The descriptor
						creator identifies an ‘entity’ (a person, group of persons or organisation)
						primarily responsible for creating the content of the resource - behind the
						camera. Different roles may be considered as representing a creator, e.g. a
						producer, an author, etc. Creator is a sub-class of Contributor. </documentation>
					<documentation source="urn:x-fims:normativeRequirement"/>
					<documentation source="urn:x-fims:serviceDescription"/>
					<documentation source="urn:x-fims:contentOfServiceDescription"/>
					<documentation source="urn:x-fims:introducedInVersion">Introduced In Version:
						1_0_7</documentation>
				</annotation>
			</element>
			<element name="subject" type="desc:subjectType" minOccurs="0" maxOccurs="unbounded">
				<annotation>
					<documentation source="urn:x-fims:description" xml:lang="en">The generalised
						topic that represents the intellectual content of the resource. Typically, a
						subject is expressed by keywords, key phrases. Free text, controlled
						vocabularies, authorities, or formal classification schemes (codes) may be
						employed when selecting descriptive subject terms. Persons as subjects are
						also placed here.</documentation>
					<documentation source="urn:x-fims:normativeRequirement"/>
					<documentation source="urn:x-fims:serviceDescription"/>
					<documentation source="urn:x-fims:contentOfServiceDescription"/>
					<documentation source="urn:x-fims:introducedInVersion">Introduced In Version:
						1_0_7</documentation>
				</annotation>
			</element>
			<element name="description" type="desc:descriptionType" minOccurs="0"
				maxOccurs="unbounded">
				<annotation>
					<documentation source="urn:x-fims:description" xml:lang="en">Free-form text or a
						narrative to report general notes, abstracts, or summaries about the
						intellectual content of a resource.</documentation>
					<documentation source="urn:x-fims:normativeRequirement"/>
					<documentation source="urn:x-fims:serviceDescription"/>
					<documentation source="urn:x-fims:contentOfServiceDescription"/>
					<documentation source="urn:x-fims:introducedInVersion">Introduced In Version:
						1_0_7</documentation>
				</annotation>
			</element>
			<element name="publisher" type="desc:entityType" minOccurs="0" maxOccurs="unbounded">
				<annotation>
					<documentation source="urn:x-fims:description" xml:lang="en">A publisher is a
						person, an organization, or a service. Typically, the name of a Publisher
						should be used to indicate the entity primarily responsible for distributing
						or making a resource available to others e.g. by broadcasting, selling,
						leasing, renting and other modes of distribution.</documentation>
					<documentation source="urn:x-fims:normativeRequirement"/>
					<documentation source="urn:x-fims:serviceDescription"/>
					<documentation source="urn:x-fims:contentOfServiceDescription"/>
					<documentation source="urn:x-fims:introducedInVersion">Introduced In Version:
						1_0_7</documentation>
				</annotation>
			</element>
			<element name="contributor" type="desc:entityType" minOccurs="0" maxOccurs="unbounded">
				<annotation>
					<documentation source="urn:x-fims:description" xml:lang="en">The descriptor
						contributor identifies a person or organization that has made substantial
						creative contributions to the content of a resource. Refers particularly
						(but not only) to participation in front of the camera. If in doubt whether
						an entity is a creator or contributor use the element
						contributor.</documentation>
					<documentation source="urn:x-fims:normativeRequirement"/>
					<documentation source="urn:x-fims:serviceDescription"/>
					<documentation source="urn:x-fims:contentOfServiceDescription"/>
					<documentation source="urn:x-fims:introducedInVersion">Introduced In Version:
						1_0_7</documentation>
				</annotation>
			</element>
			<element name="date" type="desc:dateType" minOccurs="0" maxOccurs="unbounded">
				<annotation>
					<documentation source="urn:x-fims:description" xml:lang="en">Dates associated
						with events occurring during the life of the resource. Typically, Date will
						be associated with the creation, modification or availability of the
						resource.</documentation>
					<documentation source="urn:x-fims:normativeRequirement"/>
					<documentation source="urn:x-fims:serviceDescription"/>
					<documentation source="urn:x-fims:contentOfServiceDescription"/>
					<documentation source="urn:x-fims:introducedInVersion">Introduced In Version:
						1_0_7</documentation>
				</annotation>
			</element>
			<element name="type" type="desc:typeType" minOccurs="0" maxOccurs="unbounded">
				<annotation>
					<documentation source="urn:x-fims:description" xml:lang="en">The nature or genre
						of the editorial object. Type includes terms describing general categories,
						functions, genres, or aggregation levels for content. Recommended best
						practice is to select a value from a controlled vocabulary.</documentation>
					<documentation source="urn:x-fims:normativeRequirement"/>
					<documentation source="urn:x-fims:serviceDescription"/>
					<documentation source="urn:x-fims:contentOfServiceDescription"/>
					<documentation source="urn:x-fims:introducedInVersion">Introduced In Version:
						1_0_7</documentation>
				</annotation>
			</element>
			<element name="identifier" type="desc:identifierType" minOccurs="0"
				maxOccurs="unbounded">
				<annotation>
					<documentation source="urn:x-fims:description" xml:lang="en">A unique,
						unambiguous reference or identifier for a resource within a given context.
						Best practice is to identify the resource (whether analogue or digital) by
						means of a string or number corresponding to an established or formal
						identification system if one exists. Otherwise, use an identification method
						that is in use within your agency, station, production company, office, or
						institution. It is also possible to enter more than one, different but still
						unique, identifier for the same resource.</documentation>
					<documentation source="urn:x-fims:normativeRequirement"/>
					<documentation source="urn:x-fims:serviceDescription"/>
					<documentation source="urn:x-fims:contentOfServiceDescription"/>
					<documentation source="urn:x-fims:introducedInVersion">Introduced In Version:
						1_0_7</documentation>
				</annotation>
			</element>
			<element name="language" type="desc:languageType" minOccurs="0" maxOccurs="unbounded">
				<annotation>
					<documentation source="urn:x-fims:description" xml:lang="en">Identifies
						languages and their use in the intellectual content of the resource.
						Recommended best practice for the values of the Language element is defined
						by RFC 5646. It is recommended to use a two-letter Language Code (taken from
						the ISO Standard 639), followed optionally, by a two-letter Country Code
						(taken from the ISO Standard 3166) preferably expressed using capitals
						letters. For example, 'en' for English, 'fr' for French, or 'en-UK' for
						English used in the United Kingdom. More contextual information can be
						provided using the “note” attribute.</documentation>
					<documentation source="urn:x-fims:normativeRequirement"/>
					<documentation source="urn:x-fims:serviceDescription"/>
					<documentation source="urn:x-fims:contentOfServiceDescription"/>
					<documentation source="urn:x-fims:introducedInVersion">Introduced In Version:
						1_0_7</documentation>
				</annotation>
			</element>
			<element name="relation" type="desc:relationType" minOccurs="0" maxOccurs="unbounded">
				<annotation>
					<documentation source="urn:x-fims:description" xml:lang="en">Recommended best
						practice is to reference the resource (to which the current resource under
						description is related) by means of a string or number conforming to a
						formal identification system. Relation is used to show the relation in
						content to another resource. For example, "IsPartOf" is used to show the
						relation between a part of a radio programme and the whole programme or
						between a track and a record album. A resource can be identified by its
						title, or an identifier (possibly a URI). The related item has its own
						separate metadata record. Relation is used to provide a name, an
						identification number or ID, or a locator where the related item can be
						found.</documentation>
					<documentation source="urn:x-fims:normativeRequirement"/>
					<documentation source="urn:x-fims:serviceDescription"/>
					<documentation source="urn:x-fims:contentOfServiceDescription"/>
					<documentation source="urn:x-fims:introducedInVersion">Introduced In Version:
						1_1_0</documentation>
				</annotation>
			</element>
			<element name="isVersionOf" type="desc:relationType" minOccurs="0" maxOccurs="unbounded">
				<annotation>
					<documentation source="urn:x-fims:description" xml:lang="en">A reference to the
						resource that the current resource is a version of </documentation>
					<documentation source="urn:x-fims:normativeRequirement"/>
					<documentation source="urn:x-fims:serviceDescription"/>
					<documentation source="urn:x-fims:contentOfServiceDescription"/>
					<documentation source="urn:x-fims:introducedInVersion">Introduced In Version:
						1_1_0</documentation>
				</annotation>
			</element>
			<element name="hasVersion" type="desc:relationType" minOccurs="0" maxOccurs="unbounded">
				<annotation>
					<documentation source="urn:x-fims:description" xml:lang="en">A reference to
						another version of the resource </documentation>
					<documentation source="urn:x-fims:normativeRequirement"/>
					<documentation source="urn:x-fims:serviceDescription"/>
					<documentation source="urn:x-fims:contentOfServiceDescription"/>
					<documentation source="urn:x-fims:introducedInVersion">Introduced In Version:
						1_1_0</documentation>
				</annotation>
			</element>
			<element name="isReplacedBy" type="desc:relationType" minOccurs="0"
				maxOccurs="unbounded">
				<annotation>
					<documentation source="urn:x-fims:description" xml:lang="en">A reference to a
						resource replacing the current resource</documentation>
					<documentation source="urn:x-fims:normativeRequirement"/>
					<documentation source="urn:x-fims:serviceDescription"/>
					<documentation source="urn:x-fims:contentOfServiceDescription"/>
					<documentation source="urn:x-fims:introducedInVersion">Introduced In Version:
						1_1_0</documentation>
				</annotation>
			</element>
			<element name="replaces" type="desc:relationType" minOccurs="0" maxOccurs="unbounded">
				<annotation>
					<documentation source="urn:x-fims:description" xml:lang="en">A reference to a
						resource that the current resource replaces</documentation>
					<documentation source="urn:x-fims:normativeRequirement"/>
					<documentation source="urn:x-fims:serviceDescription"/>
					<documentation source="urn:x-fims:contentOfServiceDescription"/>
					<documentation source="urn:x-fims:introducedInVersion">Introduced In Version:
						1_1_0</documentation>
				</annotation>
			</element>
			<element name="isRequiredBy" type="desc:relationType" minOccurs="0"
				maxOccurs="unbounded">
				<annotation>
					<documentation source="urn:x-fims:description" xml:lang="en">A reference to a
						resource requiring the current resource</documentation>
					<documentation source="urn:x-fims:normativeRequirement"/>
					<documentation source="urn:x-fims:serviceDescription"/>
					<documentation source="urn:x-fims:contentOfServiceDescription"/>
					<documentation source="urn:x-fims:introducedInVersion">Introduced In Version:
						1_1_0</documentation>
				</annotation>
			</element>
			<element name="requires" type="desc:relationType" minOccurs="0" maxOccurs="unbounded">
				<annotation>
					<documentation source="urn:x-fims:description" xml:lang="en">A reference to a
						resource that the current resource requires</documentation>
					<documentation source="urn:x-fims:normativeRequirement"/>
					<documentation source="urn:x-fims:serviceDescription"/>
					<documentation source="urn:x-fims:contentOfServiceDescription"/>
					<documentation source="urn:x-fims:introducedInVersion">Introduced In Version:
						1_1_0</documentation>
				</annotation>
			</element>
			<element name="isPartOf" type="desc:relationType" minOccurs="0" maxOccurs="unbounded">
				<annotation>
					<documentation source="urn:x-fims:description" xml:lang="en">A reference to a
						resource that the current resource is a part of</documentation>
					<documentation source="urn:x-fims:normativeRequirement"/>
					<documentation source="urn:x-fims:serviceDescription"/>
					<documentation source="urn:x-fims:contentOfServiceDescription"/>
					<documentation source="urn:x-fims:introducedInVersion">Introduced In Version:
						1_1_0</documentation>
				</annotation>
			</element>
			<element name="hasPart" type="desc:relationType" minOccurs="0" maxOccurs="unbounded">
				<annotation>
					<documentation source="urn:x-fims:description" xml:lang="en"> A reference to a
						resource that forms part of the current resource </documentation>
					<documentation source="urn:x-fims:normativeRequirement"/>
					<documentation source="urn:x-fims:serviceDescription"/>
					<documentation source="urn:x-fims:contentOfServiceDescription"/>
					<documentation source="urn:x-fims:introducedInVersion">Introduced In Version:
						1_1_0</documentation>
				</annotation>
			</element>
			<element name="isReferencedBy" type="desc:relationType" minOccurs="0"
				maxOccurs="unbounded">
				<annotation>
					<documentation source="urn:x-fims:description" xml:lang="en">A reference to a
						resource that references the current resource</documentation>
					<documentation source="urn:x-fims:normativeRequirement"/>
					<documentation source="urn:x-fims:serviceDescription"/>
					<documentation source="urn:x-fims:contentOfServiceDescription"/>
					<documentation source="urn:x-fims:introducedInVersion">Introduced In Version:
						1_1_0</documentation>
				</annotation>
			</element>
			<element name="references" type="desc:relationType" minOccurs="0" maxOccurs="unbounded">
				<annotation>
					<documentation source="urn:x-fims:description" xml:lang="en">A reference to a
						resource that the current resource references</documentation>
					<documentation source="urn:x-fims:normativeRequirement"/>
					<documentation source="urn:x-fims:serviceDescription"/>
					<documentation source="urn:x-fims:contentOfServiceDescription"/>
					<documentation source="urn:x-fims:introducedInVersion">Introduced In Version:
						1_1_0</documentation>
				</annotation>
			</element>
			<element name="isFormatOf" type="desc:relationType" minOccurs="0" maxOccurs="unbounded">
				<annotation>
					<documentation source="urn:x-fims:description" xml:lang="en">A reference to a
						resource with which the current resource shares a format </documentation>
					<documentation source="urn:x-fims:normativeRequirement"/>
					<documentation source="urn:x-fims:serviceDescription"/>
					<documentation source="urn:x-fims:contentOfServiceDescription"/>
					<documentation source="urn:x-fims:introducedInVersion">Introduced In Version:
						1_1_0</documentation>
				</annotation>
			</element>
			<element name="hasFormat" type="desc:relationType" minOccurs="0" maxOccurs="unbounded">
				<annotation>
					<documentation source="urn:x-fims:description" xml:lang="en">A format in which
						the resource is also available</documentation>
					<documentation source="urn:x-fims:normativeRequirement"/>
					<documentation source="urn:x-fims:serviceDescription"/>
					<documentation source="urn:x-fims:contentOfServiceDescription"/>
					<documentation source="urn:x-fims:introducedInVersion">Introduced In Version:
						1_1_0</documentation>
				</annotation>
			</element>
			<element name="isRelatedTo" type="desc:relationType" minOccurs="0" maxOccurs="unbounded">
				<annotation>
					<documentation source="urn:x-fims:description" xml:lang="en">A reference to a
						resource with which the current resource is related to</documentation>
					<documentation source="urn:x-fims:normativeRequirement"/>
					<documentation source="urn:x-fims:serviceDescription"/>
					<documentation source="urn:x-fims:contentOfServiceDescription"/>
					<documentation source="urn:x-fims:introducedInVersion">Introduced In Version:
						1_2_0</documentation>
				</annotation>
			</element>
			<element name="isEpisodeOf" type="desc:relationType" minOccurs="0" maxOccurs="unbounded">
				<annotation>
					<documentation source="urn:x-fims:description" xml:lang="en">A reference to a
						series the current resource is an episode of</documentation>
					<documentation source="urn:x-fims:normativeRequirement"/>
					<documentation source="urn:x-fims:serviceDescription"/>
					<documentation source="urn:x-fims:contentOfServiceDescription"/>
					<documentation source="urn:x-fims:introducedInVersion">Introduced In Version:
						1_1_0</documentation>
				</annotation>
			</element>
			<element name="hasEpisode" type="desc:relationType" minOccurs="0" maxOccurs="unbounded">
				<annotation>
					<documentation source="urn:x-fims:description" xml:lang="en">A reference to an
						episode in a series of a season</documentation>
					<documentation source="urn:x-fims:normativeRequirement"/>
					<documentation source="urn:x-fims:serviceDescription"/>
					<documentation source="urn:x-fims:contentOfServiceDescription"/>
					<documentation source="urn:x-fims:introducedInVersion">Introduced In Version:
						1_2_0</documentation>
				</annotation>
			</element>
			<element name="isSeriesOf" type="desc:relationType" minOccurs="0" maxOccurs="unbounded">
				<annotation>
					<documentation source="urn:x-fims:description" xml:lang="en">A reference to a
						brand to which belongs a series</documentation>
					<documentation source="urn:x-fims:normativeRequirement"/>
					<documentation source="urn:x-fims:serviceDescription"/>
					<documentation source="urn:x-fims:contentOfServiceDescription"/>
					<documentation source="urn:x-fims:introducedInVersion">Introduced In Version:
						1_2_0</documentation>
				</annotation>
			</element>
			<element name="hasSeries" type="desc:relationType" minOccurs="0" maxOccurs="unbounded">
				<annotation>
					<documentation source="urn:x-fims:description" xml:lang="en">A reference to a
						series belonging to a brand.</documentation>
					<documentation source="urn:x-fims:normativeRequirement"/>
					<documentation source="urn:x-fims:serviceDescription"/>
					<documentation source="urn:x-fims:contentOfServiceDescription"/>
					<documentation source="urn:x-fims:introducedInVersion">Introduced In Version:
						1_2_0</documentation>
				</annotation>
			</element>
			<element name="isSeasonOf" type="desc:relationType" minOccurs="0" maxOccurs="unbounded">
				<annotation>
					<documentation source="urn:x-fims:description" xml:lang="en">A reference to a
						series to which belongs a season</documentation>
					<documentation source="urn:x-fims:normativeRequirement"/>
					<documentation source="urn:x-fims:serviceDescription"/>
					<documentation source="urn:x-fims:contentOfServiceDescription"/>
					<documentation source="urn:x-fims:introducedInVersion">Introduced In Version:
						1_2_0</documentation>
				</annotation>
			</element>
			<element name="hasSeason" type="desc:relationType" minOccurs="0" maxOccurs="unbounded">
				<annotation>
					<documentation source="urn:x-fims:description" xml:lang="en">A reference to a
						season belonging to a series.</documentation>
					<documentation source="urn:x-fims:normativeRequirement"/>
					<documentation source="urn:x-fims:serviceDescription"/>
					<documentation source="urn:x-fims:contentOfServiceDescription"/>
					<documentation source="urn:x-fims:introducedInVersion">Introduced In Version:
						1_2_0</documentation>
				</annotation>
			</element>
			<element name="isNextInSequence" type="desc:relationType" minOccurs="0"
				maxOccurs="unbounded">
				<annotation>
					<documentation source="urn:x-fims:description" xml:lang="en">A reference to a
						resource that comes next in an ordered sequence. </documentation>
					<documentation source="urn:x-fims:normativeRequirement"/>
					<documentation source="urn:x-fims:serviceDescription"/>
					<documentation source="urn:x-fims:contentOfServiceDescription"/>
					<documentation source="urn:x-fims:introducedInVersion">Introduced In Version:
						1_2_0</documentation>
				</annotation>
			</element>
			<element name="followsInSequence" type="desc:relationType" minOccurs="0"
				maxOccurs="unbounded">
				<annotation>
					<documentation source="urn:x-fims:description" xml:lang="en">A reference to a
						resource that comes before the current resource in an ordered sequence. </documentation>
					<documentation source="urn:x-fims:normativeRequirement"/>
					<documentation source="urn:x-fims:serviceDescription"/>
					<documentation source="urn:x-fims:contentOfServiceDescription"/>
					<documentation source="urn:x-fims:introducedInVersion">Introduced In Version:
						1_2_0</documentation>
				</annotation>
			</element>
			<element name="sameAs" type="desc:relationType" minOccurs="0" maxOccurs="unbounded">
				<annotation>
					<documentation source="urn:x-fims:description" xml:lang="en">To express a strict
						equivalence between resources. </documentation>
					<documentation source="urn:x-fims:normativeRequirement"/>
					<documentation source="urn:x-fims:serviceDescription"/>
					<documentation source="urn:x-fims:contentOfServiceDescription"/>
					<documentation source="urn:x-fims:introducedInVersion">Introduced In Version:
						1_2_0</documentation>
				</annotation>
			</element>
			<element name="isMemberOf" type="desc:relationType" minOccurs="0" maxOccurs="unbounded">
				<annotation>
					<documentation source="urn:x-fims:description" xml:lang="en">A reference to a
						group e.g. a brand, the current resource is an member of </documentation>
					<documentation source="urn:x-fims:normativeRequirement"/>
					<documentation source="urn:x-fims:serviceDescription"/>
					<documentation source="urn:x-fims:contentOfServiceDescription"/>
					<documentation source="urn:x-fims:introducedInVersion">Introduced In Version:
						1_1_0</documentation>
				</annotation>
			</element>
			<element name="hasMember" type="desc:relationType" minOccurs="0" maxOccurs="unbounded">
				<annotation>
					<documentation source="urn:x-fims:description" xml:lang="en"> A reference to one
						or more members of a group </documentation>
					<documentation source="urn:x-fims:normativeRequirement"/>
