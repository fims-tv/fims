	<simpleType name="RepositoryStateTypes">
		<annotation>
			<documentation source="urn:x-fims:description">
				The state of the repository system as reported by the GetRepositoryState RCR operation.
			</documentation>
			<documentation source="urn:x-fims:normativeRequirement"></documentation>
			<documentation source="urn:x-fims:serviceDescription">Service Description: Required</documentation>
			<documentation source="urn:x-fims:contentOfServiceDescription">Description: 
				The state of the repository system as reported by the GetRepositoryState RCR operation.
			</documentation>
		</annotation>
		<restriction base="string">
			<enumeration value="online">
				<annotation>
					<documentation source="urn:x-fims:description">
						The system is active, ready and online.
					</documentation>
				</annotation>
			</enumeration>
			
			<enumeration value="offline">
				<annotation>
					<documentation source="urn:x-fims:description">
						The system is taken offline, and not ready to receive or respond to any operations.
					</documentation>
				</annotation>
			</enumeration>
			
			<enumeration value="failover">
				<annotation>
					<documentation source="urn:x-fims:description">
						The system is faulted and in a failover state to a backup system.
					</documentation>
				</annotation>
			</enumeration>
		</restriction>
	</simpleType>

	<simpleType name="FormatProfileMetadataProcessingType">
		<annotation>
			<documentation source="urn:x-fims:description">
				Defines the repository system behavior for adding a new FormatType 
				as part of the AddEssence operation.
				Retrieved from the RCR GetGeneralCapability operation.
			</documentation>
			<documentation source="urn:x-fims:normativeRequirement"></documentation>
			<documentation source="urn:x-fims:serviceDescription">Service Description: Required</documentation>
			<documentation source="urn:x-fims:contentOfServiceDescription">Description: 
				Defines the repository system behavior for adding a new FormatType 
				as part of the AddEssence operation.
				Retrieved from the RCR GetGeneralCapability operation.
			</documentation>
		</annotation>
		<restriction base="string">
			<enumeration value="mustBeProvided">
				<annotation>
					<documentation source="urn:x-fims:description">
						The system requires the format to be submitted as part of the AddEssence operation.
					</documentation>
				</annotation>
			</enumeration>

			<enumeration value="autoGenerated">
				<annotation>
					<documentation source="urn:x-fims:description">
						The system will generate the format and it shouldn't be submitted as part of the AddEssence operation.
					</documentation>
				</annotation>
			</enumeration>

			<enumeration value="autoGeneratedWhenNotProvided">
				<annotation>
					<documentation source="urn:x-fims:description">
						The system will generate the format if it is not submitted as part of the AddEssence operation.
					</documentation>
				</annotation>
			</enumeration>
		</restriction>
	</simpleType>
	
