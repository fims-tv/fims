	<simpleType name="QueryResultInfoType">
		<annotation>
			<documentation source="urn:x-fims:description">
				Defines the content data populated properties for the matched contents resulting 
				from the ContentQuery operation.
			</documentation>
			<documentation source="urn:x-fims:normativeRequirement"></documentation>
			<documentation source="urn:x-fims:serviceDescription">Service Description: Required</documentation>
			<documentation source="urn:x-fims:contentOfServiceDescription">Description: 
				Defines the content data populated properties for the matched contents resulting 
				from the ContentQuery operation.
			</documentation>
		</annotation>
		<restriction base="string">
			<enumeration value="noItems">
				<annotation>
					<documentation source="urn:x-fims:description">
						No content returned only a count of the matching contents returned.
					</documentation>
				</annotation>
			</enumeration>
			
			<enumeration value="full">
				<annotation>
					<documentation source="urn:x-fims:description">
						The full presentation of the matching contents will be returned.
					</documentation>
				</annotation>
			</enumeration>
			
			<enumeration value="light">
				<annotation>
					<documentation source="urn:x-fims:description">
						The minimum representation of the matching contents will be returned
						(min. the resourceID, and the revisionID if the system supports versioning)
					</documentation>
				</annotation>
			</enumeration>
			
			<enumeration value="metadata">
				<annotation>
					<documentation source="urn:x-fims:description">
						The query will return the matching contents with the metadata fields only populated.
					</documentation>
				</annotation>
			</enumeration>
			
			<enumeration value="physical">
				<annotation>
					<documentation source="urn:x-fims:description">
						The query will return the matching contents with the physical (essence) fields only populated.
					</documentation>
				</annotation>
			</enumeration>
			
			<enumeration value="specific">
				<annotation>
					<documentation source="urn:x-fims:description">
						The query will return the matching contents with the specific pre-defined fields only populated.
						The specific filter is defined as part of the RCR.
					</documentation>
				</annotation>
			</enumeration>
		</restriction>
	</simpleType>
	
