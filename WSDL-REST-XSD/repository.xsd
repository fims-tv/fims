	<simpleType name="CancelOperationStatusType">
		<annotation>
			<documentation source="urn:x-fims:description">
				The status of asynchronous operation cancellation. 
				Returned in the acknowledgment, notification and fault notification for cancel operation.  
			</documentation>
			<documentation source="urn:x-fims:normativeRequirement"></documentation>
			<documentation source="urn:x-fims:serviceDescription">Service Description: Required</documentation>
			<documentation source="urn:x-fims:contentOfServiceDescription">Description: 
				The status of asynchronous operation cancellation. 
				Returned in the acknowledgment, notification and fault notification for cancel operation.
			</documentation>
		</annotation>
		<restriction base="string">
			<enumeration value="inProgress">
				<annotation>
					<documentation source="urn:x-fims:description">
						The system is processing the cancel operation.
					</documentation>
				</annotation>
			</enumeration>
			
			<enumeration value="completed">
				<annotation>
					<documentation source="urn:x-fims:description">
						The system completed successfully the cancel operation.
					</documentation>
				</annotation>
			</enumeration>
			
			<enumeration value="failed">
				<annotation>
					<documentation source="urn:x-fims:description">
						The system failed to perform the cancel operation.
					</documentation>
				</annotation>
			</enumeration>
		</restriction>
	</simpleType>

	<!-- **************************** entities **************************** -->
	<element name="repositoryFault" type="rps:RepositoryFaultType">
		<annotation>
			<documentation source="urn:x-fims:description">
				Fault information for the repository media service.
			</documentation>
			<documentation source="urn:x-fims:normativeRequirement">
				If an exception is generated when a request message is submitted to the service, 
				it shall respond with a message based on the RepositoryFaultType.
			</documentation>
			<documentation source="urn:x-fims:serviceDescription"></documentation>
			<documentation source="urn:x-fims:contentOfServiceDescription"></documentation>
		</annotation>
	</element>
	<complexType name="RepositoryFaultType">
		<annotation>
			<documentation source="urn:x-fims:description">
				Fault information for the repository media service.
			</documentation>
			<documentation source="urn:x-fims:normativeRequirement">
				If an exception is generated when a request message is submitted to the service, 
				it shall respond with a message based on the RepositoryFaultType.
			</documentation>
			<documentation source="urn:x-fims:serviceDescription"></documentation>
			<documentation source="urn:x-fims:contentOfServiceDescription"></documentation>
		</annotation>
		<complexContent>
			<extension base="bms:FaultType">
				<sequence>
					<element name="extendedCode" type="rps:RepositoryErrorCodeType" minOccurs="0" maxOccurs="1">
						<annotation>
							<documentation source="urn:x-fims:description">
								Repository service-specific error codes, as detailed in the RepositoryErrorCodeType.
							</documentation>
							<documentation source="urn:x-fims:normativeRequirement">
								This element shall be set if the service needs to provide service-specific error codes 
								which are not specified in the fault 'code' element.
								In this case, the code element shall be set to 'EXT_S00_0000' and
								the extendedCode shall be set to one of the codes described in
								RepositoryErrorCodeType. If the 'code'; element is set
								to any other value than 'EXT_S00_0000'; then this element
								shall not be set.
							</documentation>
							<documentation source="urn:x-fims:serviceDescription"></documentation>
							<documentation source="urn:x-fims:contentOfServiceDescription"></documentation>
							<documentation source="urn:x-fims:inclusionInRequest">Inclusion In Request: Not Applicable</documentation>
							<documentation source="urn:x-fims:inclusionInResponse">Inclusion In Response: Optional</documentation>
						</annotation>
					</element>
				</sequence>
			</extension>
		</complexContent>
	</complexType>

	<element name="repositoryFaults" type="rps:RepositoryFaultsType"/>
	<complexType name="RepositoryFaultsType">
		<sequence>
			<element ref="rps:repositoryFault" minOccurs="1" maxOccurs="unbounded"/>
			<any namespace="##other" processContents="lax" minOccurs="0" maxOccurs="unbounded"/>
		</sequence>
		<anyAttribute namespace="##other" processContents="lax"/>
	</complexType>

	<element name="propertyInfo" type="rps:PropertyInfoType">
		<annotation>
			<documentation source="urn:x-fims:description">
				The property update information for a target ResourceReferenceType.
				With the action required (update, new or remove) and the new value (for update or new).
